{
    "browser_action": {
       "default_title": "MEEP"
    },
    "content_scripts": [ {
       "all_frames": false,
       //"css": [ "css파일경로" ],
       "js": [ "contentscript.js" ],
       "matches": [ "https://mail.google.com/*" ],
       "run_at": "document_end"
    } ],
    "description": "enc and dec email",
    "manifest_version": 2,
    "name": "MEEP",

    "permissions": [ "storage", "identity", "identity.email", "https://www.googleapis.com/gmail/*", "https://www.google.com/m8/feeds/contacts/*", "http://*/", "https://*/" ],
    "short_name": "MEEP",
    "version": "0.1"
 }
 