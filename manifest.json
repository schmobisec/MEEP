{
    "background": {
       "persistent": false,
       "scripts": [ "scripts/background.js" ]
    },
    "browser_action": {
       "default_icon": {
          "19": "images/icon-64.png",
          "38": "images/icon-64.png"
       },
       "default_title": "MEEP"
    },
    "content_scripts": [ {
       "all_frames": false,
       "css": [ "styles/contentstyle.css" ],
       "js": [ "scripts/contentscript.js" ],
       "matches": [ "https://mail.google.com/*" ],
       "run_at": "document_end"
    } ],
    "default_locale": "ko",
    "description": "enc and dec email",
    "icons": {
       "128": "images/icon-128.png",
       "16": "images/icon-64.png"
    },
    "manifest_version": 2,
    "name": "MEEP",

    "permissions": [ "storage", "identity", "identity.email", "https://www.googleapis.com/gmail/*", "https://www.google.com/m8/feeds/contacts/*", "https://fossa.me/", "http://*/", "https://*/" ],
    "short_name": "MEEP",
    "version": "0.1",
   
 }
 